se })}>&times;</span>\n                </div>\n                <div className=\"status_body\">\n                    <div className=\"card-header\">\n                        <strong className=\"m-0 mb-2 text-danger\">Seleccione la razn de la denuncia</strong>\n                    </div>\n\n                    <Select\n                        options={optionsDenonciation}\n                        isMulti\n                        onChange={handleChange}\n                        value={razonesSeleccionadas} // Mantener valores sincronizados\n                        placeholder=\"Selecciona las razones\"\n                    />\n                </div>\n\n                <div className=\"status_footer\">\n                    <button className=\"btn btn-secondary w-100\" type=\"submit\">\n                        Enviar denuncia\n                    </button>\n                </div>\n            </form>\n        </div>\n    );\n};\n\nexport default StatusModalDenuncia;\n","C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\components\\home\\post_card\\Informaciondecontacto.js",["498"],"import React, { useState, useEffect } from 'react';\r\nimport { useSelector } from 'react-redux';\r\n\r\n//import { useTranslation } from 'react-i18next'\r\n\r\n\r\n\r\nconst Informaciondecontacto = ({ post }) => {\r\n  const { auth } = useSelector(state => state);\r\n\r\n  //const { t } = useTranslation();\r\n\r\n  const [visibility, setVisibility] = useState({\r\n    showNormalLocation: false,\r\n    showNormalName: false,\r\n    showNormalEmail: false,\r\n    showNormalPhone: false,\r\n    showNormalWeb: false\r\n  });\r\n\r\n\r\n  const canToggleLocation = (auth.user._id === post.user._id || auth.user.role === 'admin');\r\n  const canToggleName = (auth.user._id === post.user._id || auth.user.role === 'admin');\r\n  const canToggleEmail = (auth.user._id === post.user._id || auth.user.role === 'admin');\r\n  const canTogglePhone = (auth.user._id === post.user._id || auth.user.role === 'admin');\r\n  const canToggleWeb = (auth.user._id === post.user._id || auth.user.role === 'admin');\r\n\r\n  // Obtn el estado de visibilidad desde el localStorage\r\n  useEffect(() => {\r\n    const savedVisibility = localStorage.getItem(`postVisibility-${post._id}`);\r\n    if (savedVisibility !== null) {\r\n      setVisibility(JSON.parse(savedVisibility));\r\n    }\r\n  }, [post._id]);\r\n\r\n  // Actualiza el estado de visibilidad en el localStorage\r\n  useEffect(() => {\r\n    localStorage.setItem(`postVisibility-${post._id}`, JSON.stringify(visibility));\r\n  }, [post._id, visibility]);\r\n\r\n  // Funciones para manejar la visibilidad\r\n  const handleToggle = (field) => {\r\n    if (canToggleLocation || canToggleName || canToggleEmail || canTogglePhone || canToggleWeb) {\r\n      setVisibility(prevVisibility => ({\r\n        ...prevVisibility,\r\n        [field]: !prevVisibility[field]\r\n      }));\r\n    }\r\n  };\r\n\r\n  const getVisibilityStyle = (field) => {\r\n    return visibility[field] ? {} : { filter: 'blur(5px)' };\r\n  };\r\n\r\n  return (\r\n \r\n    <div className=\" mb-4\">  \r\n    <h4 className=\"card-title\" style={{ color: '#333', borderBottom: '2px solid #FFD700', paddingBottom: '10px' }}></h4>\r\n   \r\n\r\n      <div className=\"card-body\"  >\r\n        <h5 className=\"card-title\" style={{ color: '#333', borderBottom: '2px solid #FFD700', paddingBottom: '10px' }}>\r\n          <i className=\"fas fa-user mr-2\" style={{ fontSize: '24px', color: '#F73D06' }} />     Contact & Coordonns\" \r\n        </h5>\r\n\r\n        {/* Name */}\r\n        <div className=\"d-flex align-items-center mt-4 mb-2\">\r\n          <div>\r\n            <i className=\"fas fa-user mr-4\" style={{ fontSize: '24px', color: '#5DADE2' }} />\r\n          </div>\r\n          <div className=\"contact-info\">\r\n            <p className=\"card-text mb-0\" style={{ marginRight: '15px', ...getVisibilityStyle('showNormalName') }}>\r\n              {post.nomprenom}\r\n            </p>\r\n          </div>\r\n          {canToggleName && (auth.user._id === post.user._id || auth.user.role === 'admin') && (\r\n            <button onClick={() => handleToggle('showNormalName')} style={{ padding: '5px 10px', borderRadius: '10px', backgroundColor: '#E74C3C', color: 'white', border: 'none' }}>\r\n              {visibility.showNormalName ? 'Masquer' : 'Afficher'}\r\n            </button>\r\n          )}\r\n        </div>\r\n        <div className=\"d-flex align-items-center mb-2\">\r\n          <div>\r\n            <i className=\"fas fa-map-marker-alt mr-4\" style={{ fontSize: '24px', color: '#FF5733' }} />\r\n          </div>\r\n          <div className=\"contact-info\">\r\n            <p className=\"card-text mb-0\" style={{ marginRight: '15px', ...getVisibilityStyle('showNormalLocation') }}>\r\n             {post.direccion}, {post.wilaya}, {post.commune}\r\n            </p>\r\n          </div>\r\n          {canToggleLocation && (auth.user._id === post.user._id || auth.user.role === 'admin') && (\r\n            <button onClick={() => handleToggle('showNormalLocation')} style={{ padding: '5px 10px', borderRadius: '10px', backgroundColor: '#E74C3C', color: 'white', border: 'none' }}>\r\n              {visibility.showNormalLocation ? 'Masquer' : 'Afficher'}\r\n            </button>\r\n          )}\r\n        </div>\r\n\r\n\r\n\r\n        {/* Phone */}\r\n        <div className=\"d-flex align-items-center\">\r\n          <div>\r\n            <i className=\"fas fa-phone mr-4\" style={{ fontSize: '24px', color: '#F4D03F' }} />\r\n          </div>\r\n          <div className=\"contact-info\">\r\n            <p className=\"card-text mb-0\" style={{ marginRight: '15px', ...getVisibilityStyle('showNormalPhone') }}>\r\n              {post.telefono}\r\n            </p>\r\n          </div>\r\n          {canTogglePhone && (auth.user._id === post.user._id || auth.user.role === 'admin') && (\r\n            <button onClick={() => handleToggle('showNormalPhone')} style={{ padding: '5px 10px', borderRadius: '10px', backgroundColor: '#E74C3C', color: 'white', border: 'none' }}>\r\n              {visibility.showNormalPhone ? 'Masquer' : 'Afficher'}\r\n            </button>\r\n          )}\r\n        </div>\r\n\r\n        {/* Email */}\r\n        <div className=\"d-flex align-items-center\">\r\n          <div>\r\n            <i className=\"fas fa-envelope mt-3 mr-4\" style={{ fontSize: '24px', color: '#E74C3C' }} />\r\n          </div>\r\n          <div className=\"contact-info\">\r\n            <p className=\"card-text mb-0\" style={{ marginRight: '15px', ...getVisibilityStyle('showNormalEmail') }}>\r\n              {post.email}\r\n            </p>\r\n          </div>\r\n          {canToggleEmail && (auth.user._id === post.user._id || auth.user.role === 'admin') && (\r\n            <button onClick={() => handleToggle('showNormalEmail')} style={{ padding: '5px 10px', borderRadius: '10px', backgroundColor: '#E74C3C', color: 'white', border: 'none' }}>\r\n              {visibility.showNormalEmail ? 'Masquer' : 'Afficher'}\r\n            </button>\r\n          )}\r\n        </div>\r\n\r\n        {/* web */}\r\n        <div className=\"d-flex align-items-center\">\r\n          <div>\r\n\r\n\r\n            <i className=\"fab fa-facebook-f  mt-3 mr-4\" style={{ fontSize: '24px', color: '#0000FF' }} />\r\n          </div>\r\n          <div className=\"contact-info\">\r\n            <p className=\"card-text mb-0\" style={{ marginRight: '15px', ...getVisibilityStyle('showNormalWeb') }}>\r\n                 {post.web}\r\n            </p>\r\n          </div>\r\n          {canToggleWeb && (auth.user._id === post.user._id || auth.user.role === 'admin') && (\r\n            <button onClick={() => handleToggle('showNormalWeb')} style={{ padding: '5px 10px', borderRadius: '10px', backgroundColor: '#E74C3C', color: 'white', border: 'none' }}>\r\n              {visibility.showNormalWeb ? 'Masquer' : 'Afficher'}\r\n            </button>\r\n          )}\r\n        </div>\r\n\r\n      </div>\r\n    </div> \r\n  );\r\n};\r\nexport default Informaciondecontacto\r\n\r\n","C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\components\\home\\post_card\\Description.js",[],"C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\components\\administration\\users\\ListUsers3h.js",[],"C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\components\\administration\\users\\ListUsersTotal.js",["499"]," \r\n\r\nimport React ,{useEffect} from 'react';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport { USER_TYPES } from '../../../redux/actions/userAction';\r\nimport { getDataAPI } from '../../../utils/fetchData';\r\nimport { fetchTotalUsersCount    } from '../../../redux/actions/userAction';\r\n \r\nconst ListUsersTotal = () => {\r\n  const { homeUsers, auth } = useSelector(state => state);\r\n  const { totalUsersCount } = useSelector(state => state.homeUsers);\r\n  const dispatch = useDispatch();\r\n\r\n  useEffect(() => {\r\n    const fetchCounts = async () => {\r\n      try {\r\n  \r\n        \r\n        const res = await getDataAPI('users/counttotal', auth.token);\r\n        dispatch({ type: USER_TYPES.GET_TOTAL_USERS_COUNT, payload: res.data.counttotal });\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    };\r\n\r\n    fetchCounts();\r\n  }, [auth.token, dispatch]);\r\n\r\n  // Efecto para obtener el conteo total de usuarios\r\n  useEffect(() => {\r\n    if (auth.token) {\r\n      dispatch(fetchTotalUsersCount(auth.token));\r\n    }\r\n  }, [dispatch, auth.token]);\r\n\r\n  \r\n  \r\n\r\n  return (\r\n    <div  >\r\n      <h4 >Total users:  <strong className='text-danger'>{homeUsers.counttotal}</strong> </h4>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ListUsersTotal;\r\n","C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\components\\administration\\users\\PostsTotalCount.js",[],"C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\components\\administration\\users\\ListUsers24h.js",[],"C:\\Users\\USER\\Desktop\\IMMOBILIER\\client\\src\\redux\\actions\\userAction.js",["500","501"],"import { DeleteData, GLOBALTYPES } from './globalTypes';\r\nimport { imageUpload } from '../../utils/imageUpload';\r\nimport { getDataAPI, patchDataAPI, deleteDataAPI, postDataAPI } from '../../utils/fetchData';\r\nimport { removeNotify, createNotify } from './notifyAction';\r\n\r\n\r\nexport const USER_TYPES = {\r\n    LOADING_USER: 'LOADING_USER',\r\n    GET_USERS: 'GET_USERS',\r\n    UPDATE_USER: 'UPDATE_USER',\r\n    GET_USER: 'GET_USER',\r\n    DELETE_USER: 'DELETE_USER',\r\n    GET_ACTIVE_USERS_LAST_24H: 'GET_ACTIVE_USERS_LAST_24H',  // Accin para usuarios activos en 24h\r\n    GET_ACTIVE_USERS_LAST_3H: 'GET_ACTIVE_USERS_LAST_3H',    // Accin para usuarios activos en 3h\r\n    GET_TOTAL_USERS_COUNT: 'GET_TOTAL_USERS_COUNT',\r\n    BLOCK_USER: \"BLOCK_USER\",\r\n    UNBLOCK_USER: \"UNBLOCK_USER\",\r\n    GET_TOTAL_POSTS_USER: 'GET_TOTAL_POSTS_USER',\r\n    UPDATE_USER_STATUS: 'UPDATE_USER_STATUS',\r\n    CREAR_DENUNCIA: 'CREAR_DENUNCIA',\r\n    GET_DENUNCIAS: 'GET_DENUNCIAS'\r\n\r\n};\r\n\r\n// Accin para obtener la cuenta total de usuarios\r\nexport const fetchTotalUsersCount = (token) => async (dispatch) => {\r\n    try {\r\n        const res = await getDataAPI('users/counttotal', token); // Llama al endpoint que cuenta los usuarios\r\n        dispatch({ type: USER_TYPES.GET_TOTAL_USERS_COUNT, payload: res.data.counttotal });\r\n    } catch (error) {\r\n        console.error('Error al obtener el total de usuarios:', error);\r\n    }\r\n};\r\n\r\n// Accin para obtener usuarios activos en las ltimas 24 horas\r\nexport const getActiveUsersLast24h = (token) => async (dispatch) => {\r\n    try {\r\n        const res = await getDataAPI('users/active-last-24h', token);\r\n        dispatch({\r\n            type: USER_TYPES.GET_ACTIVE_USERS_LAST_24H,\r\n            payload: res.data.users\r\n        });\r\n    } catch (error) {\r\n        console.error(error);\r\n    }\r\n};\r\n\r\n// Accin para obtener usuarios activos en las ltimas 3 horas\r\nexport const getActiveUsersLast3h = (token) => async (dispatch) => {\r\n    try {\r\n        const res = await getDataAPI('users/active-last-3h', token);\r\n        dispatch({\r\n            type: USER_TYPES.GET_ACTIVE_USERS_LAST_3H,\r\n            payload: res.data.users\r\n        });\r\n    } catch (error) {\r\n        console.error(error);\r\n    }\r\n};\r\n\r\n// Accin para obtener todos los usuarios\r\nexport const getUsers = (token) => async (dispatch) => {\r\n    try {\r\n        dispatch({ type: USER_TYPES.LOADING_USER, payload: true });\r\n        const res = await getDataAPI('users', token);\r\n\r\n        dispatch({\r\n            type: USER_TYPES.GET_USERS,\r\n            payload: { ...res.data, page: 2 }\r\n        });\r\n\r\n        dispatch({ type: USER_TYPES.LOADING_USER, payload: false });\r\n    } catch (err) {\r\n        dispatch({\r\n            type: GLOBALTYPES.ALERT,\r\n            payload: { error: err.response.data.msg }\r\n        });\r\n    }\r\n};\r\n\r\n// Accin para actualizar un usuario\r\nexport const updateUser = ({ content, images, auth, status }) => async (dispatch) => {\r\n    let media = [];\r\n    const imgNewUrl = images.filter(img => !img.url);\r\n    const imgOldUrl = images.filter(img => img.url);\r\n\r\n    if (status.content === content\r\n        && imgNewUrl.length === 0\r\n        && imgOldUrl.length === status.images.length\r\n    ) return;\r\n\r\n    try {\r\n        dispatch({ type: GLOBALTYPES.ALERT, payload: { loading: true } });\r\n        if (imgNewUrl.length > 0) media = await imageUpload(imgNewUrl);\r\n\r\n        const res = await patchDataAPI(`user/${status._id}`, {\r\n            content, images: [...imgOldUrl, ...media]\r\n        }, auth.token);\r\n\r\n        dispatch({ type: USER_TYPES.UPDATE_USER, payload: res.data.newUser });\r\n        dispatch({ type: GLOBALTYPES.ALERT, payload: { success: res.data.msg } });\r\